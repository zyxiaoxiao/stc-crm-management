    "vue3-layer": "^1.0.18",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "vite": "^4.3.9",
    "vite-plugin-vue-setup-extend": "^0.4.0"
  }
}
